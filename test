[select new com.praksa.demo.dto.DvoristeStats( sum(weight) as mySum, count(id) as mycount, (select count(distinct user_id) as useri from com.praksa.demo.entity.Recyclation r where r.dvoriste.id =:dvoristeId ), (select count(distinct category_id) as kategorije from com.praksa.demo.entity.Recyclation r where r.dvoriste.id =:dvoristeId) (select count (distinct user_id) as novi from com.praksa.demo.entity.Recyclation r where r.created_at>:currentMonth and r.dvoriste.id =:dvoristeId and user_id not in (select distinct user_id from com.praksa.demo.entity.Recyclation r where r.created_at <:currentMonth))) from com.praksa.demo.entity.Recyclation r where r.dvoriste.id =:dvoristeId]